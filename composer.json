{
    "name": "cjango/laravel-shop",
    "description": "Shop Module for Laravel",
    "keywords": ["shop", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "cjango",
            "email": "chenjxlg@163.com"
        }
